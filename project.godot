; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Hounds"
config/description="A simple game based on the Hare and Hounds board game."
run/main_scene="res://scenes/MainMenu.tscn"
config/features=PackedStringArray("4.0", "Vulkan Mobile")
config/icon="res://icon.png"
config/quit_on_go_back=false

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/size/borderless=true
window/size/fullscreen=true
window/handheld/orientation=4
window/stretch/mode="canvas_items"

[importer_defaults]

font_data_dynamic={
"antialiased": false,
"compress": true,
"embolden": 0.0,
"face_index": 0,
"force_autohinter": false,
"generate_mipmaps": true,
"hinting": 1,
"msdf_pixel_range": 8,
"msdf_size": 48,
"multichannel_signed_distance_field": false,
"opentype_feature_overrides": {},
"oversampling": 0.0,
"preload/char_ranges": PackedStringArray(),
"preload/configurations": PackedStringArray(),
"preload/glyph_ranges": PackedStringArray(),
"subpixel_positioning": 1,
"support_overrides/language_disabled": PackedStringArray(),
"support_overrides/language_enabled": PackedStringArray(),
"support_overrides/script_disabled": PackedStringArray(),
"support_overrides/script_enabled": PackedStringArray(),
"transform": Transform2D(1, 0, 0, 1, 0, 0)
}

[input]

pause={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"window_id":0,"store_command":true,"alt_pressed":false,"shift_pressed":false,"meta_pressed":false,"command_pressed":false,"pressed":false,"keycode":0,"physical_keycode":16777217,"unicode":0,"echo":false,"script":null)
]
}

[rendering]

vulkan/rendering/back_end=1
